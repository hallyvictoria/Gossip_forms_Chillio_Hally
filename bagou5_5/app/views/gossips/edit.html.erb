<h3>Modifiez votre potin</h3>

<%= form_tag url_for(action: "update"), method: "put" do %>

  <%= label_tag 'Titre' %><br/>
  <%= text_field_tag 'Title' %>
  <br /> <br />

  <%= label_tag 'Contenu' %><br/>
  <%= text_area_tag 'Content' %>
  <br /> <br />

  <%= submit_tag "Valider" %>
<% end %>
</form>